// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model Bill {
    id        Int   @id
    title String
    url String
    description String @db.Text
    image String?
    published Int

    @@map("bill")
}
